<div class="team">

  <div class="container">
    <h3>This template is built with <i class="fa fa-heart" style="color: #FF6666"></i> by</h3>
    <div class="mate-list">
      <ul class="list-inline">
        <% data.users.each do |user| %>
        <li>
          <img src="images/<%= user.picture %>.jpeg" class="mate-avatar">
          <h2><%= user.name %></h2>
          <a href="https://github.com/<%= user.github_nickname %>"><i class="fa fa-github"></i> <%= user.github_nickname %></a>
        </li>
        <% end %>
      </div>
    </div>
  </div>
</div>
</div>
